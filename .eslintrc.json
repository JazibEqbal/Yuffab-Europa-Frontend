{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "prettier", "airbnb"],
  "plugins": ["react", "prettier"],
  "rules": {
    "react/jsx-filename-extension": "off",
    "comma-dangle": "off",
    "import/no-extraneous-dependencies" : "off",
    "no-console": "off",
    "react/no-this-in-sfc":"off"
  }
}
  