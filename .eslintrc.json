{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "prettier", "airbnb"],
  "plugins": ["react", "prettier"],
  "rules": {
    "react/jsx-filename-extension": "off",
    "comma-dangle": "off",
    "linebreak-style":["off", "windows"],
    "jsx-a11y/anchor-is-valid": "off"
  }
}
